{
  "name": "Volume Booster",
  "description": "Increase volume, beyond the maximum level, in youtube.",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "*://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "worker.js"
  },
  "content_scripts": [{
    "matches": ["*://www.youtube.com/*"],
    "js": ["data/inject.js"],
    "css": ["data/inject.css"],
    "run_at": "document_start",
    "all_frames": true
  }]
}
